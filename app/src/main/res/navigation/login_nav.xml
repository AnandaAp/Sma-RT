<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login_nav"
    app:startDestination="@id/registerRT">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.maluku.sma_rt.view.LoginChoiceFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_loginchoice"/>
    <fragment
        android:id="@+id/onboardingFragmentOne"
        android:name="com.maluku.sma_rt.view.OnboardingFragmentOne"
        android:label="fragment_onboarding_one"
        tools:layout="@layout/fragment_register_r_t" />
    <fragment
        android:id="@+id/login_RT"
        android:name="com.maluku.sma_rt.view.LoginRT"
        android:label="fragment_login__r_t"
        tools:layout="@layout/fragment_login__r_t" />
    <fragment
        android:id="@+id/registerRT"
        android:name="com.maluku.sma_rt.view.LoginRT"
        android:label="fragment_register_r_t"
        tools:layout="@layout/fragment_login__r_t">
        <action
            android:id="@+id/action_registerRT_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.maluku.sma_rt.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
</navigation>